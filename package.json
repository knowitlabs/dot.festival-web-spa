    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "conventional-changelog-conventionalcommits": "^6.1.0",
    "cssnano": "^6.0.0",
    "dotenv": "^16.0.3",
